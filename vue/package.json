    "@vitejs/plugin-vue": "^4.4.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.53.0",
    "eslint-plugin-vue": "^9.18.1",
    "npm-run-all": "^4.1.5",
    "pinia": "^2.1.7",
    "prettier": "^3.1.0",
    "typescript": "~5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.3.8",
